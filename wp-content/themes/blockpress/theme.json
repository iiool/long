{
	"$schema": "https://schemas.wp.org/wp/6.0/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "no-title",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Page without title"
		},
		{
			"name": "page-sticky",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Page with sticky columns"
		},
		{
			"name": "page-bgtitle",
			"postTypes": [
				"page"
			],
			"title": "Page with transparent header"
		},
		{
			"name": "post-bgtitle",
			"postTypes": [
				"post"
			],
			"title": "Post with transparent header"
		},
		{
			"name": "post-bgtitle-image",
			"postTypes": [
				"post"
			],
			"title": "Post with colored title and image"
		},
		{
			"name": "post-covertitle",
			"postTypes": [
				"post"
			],
			"title": "Post with cover image"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"gradients": [
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--primary) 0%,var(--wp--preset--color--secondary) 100%)",
					"name": "Diagonal primary to secondady",
					"slug": "diagonal-primary-to-secondary"
				},
				{
					"gradient": "linear-gradient(to top right,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--primary) 100%)",
					"name": "Diagonal secondary to primary",
					"slug": "diagonal-secondary-to-primary"
				},
				{
					"gradient": "linear-gradient(to top right,var(--wp--preset--color--accent) 0%,var(--wp--preset--color--button) 100%)",
					"name": "Diagonal accent to Button",
					"slug": "diagonal-accent-to-button"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--button) 0%,var(--wp--preset--color--accent) 100%)",
					"name": "Diagonal Button to accent",
					"slug": "diagonal-button-to-accent"
				},
				{
					"gradient": "linear-gradient(to top right,var(--wp--preset--color--accenttwo) 0%,var(--wp--preset--color--button) 100%)",
					"name": "Diagonal accent Secondary to Button",
					"slug": "diagonal-accenttwo-to-button"
				},
				{
					"gradient": "linear-gradient(to bottom right,var(--wp--preset--color--button) 0%,var(--wp--preset--color--accenttwo) 100%)",
					"name": "Diagonal Button to accent Secondary",
					"slug": "diagonal-button-to-accenttwo"
				}
			],
			"palette": [
				{
					"color": "#00A2E8",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#2184f9",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#fffffd",
					"name": "Text over primary",
					"slug": "textonprimary"
				},
				{
					"color": "#ffd800",
					"name": "Accented Buttons",
					"slug": "button"
				},
				{
					"color": "#010101",
					"name": "Text over buttons",
					"slug": "textonbutton"
				},
				{
					"color": "#de1414",
					"name": "Accented Color",
					"slug": "accent"
				},
				{
					"color": "#00d084",
					"name": "Accented Secondary",
					"slug": "accenttwo"
				},
				{
					"color": "#41474c",
					"name": "Accented Dark Background",
					"slug": "accentbg"
				},
				{
					"color": "#f9fafb",
					"name": "Accented Light Background",
					"slug": "lightbg"
				},
				{
					"color": "#94fe5226",
					"name": "Tag color",
					"slug": "lighttag"
				},
				{
					"color": "#b952fe26",
					"name": "Secondary tag color",
					"slug": "lightsecondarytag"
				},
				{
					"color": "#6f8099",
					"name": "Light Grey (for Meta text)",
					"slug": "lightgrey"
				},
				{
					"color": "#444444",
					"name": "Price color (for woocommerce)",
					"slug": "price"
				},
				{
					"color": "#cecece6b",
					"name": "Borders, Dividers",
					"slug": "lightborder"
				},
				{
					"color": "#444",
					"name": "Input text",
					"slug": "inputtext"
				},
				{
					"color": "#fafbfd",
					"name": "Input background",
					"slug": "inputbg"
				},
				{
					"color": "#000001",
					"name": "Dark panels (footer, header)",
					"slug": "dark"
				},
				{
					"color": "#fffffe",
					"name": "Text on dark panels",
					"slug": "textondark"
				}
			]
		},
		"custom": {
			"spacing": {
				"elements": "1.5rem",
				"large": "2rem",
				"medium": "1.5rem",
				"small": "1rem"
			},
			"typography": {
				"font-size": {
					"text": "16px"
				},
				"line-height": {
					"headings": 1.4,
					"text": 1.6
				}
			}
		},
		"layout": {
			"contentSize": "65em",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"ch"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "Barlow, Arial, Verdana",
					"name": "Barlow",
					"slug": "barlow"
				},
				{
					"fontFamily": "Inter, Arial, Verdana",
					"name": "Inter",
					"slug": "inter"
				}
			],
			"fontSizes": [
				{
					"size": "14px",
					"slug": "tiny"
				},
				{
					"size": "16px",
					"slug": "small"
				},
				{
					"size": "18px",
					"slug": "medium"
				},
				{
					"size": "24px",
					"slug": "large"
				},
				{
					"size": "26px",
					"slug": "x-large"
				},
				{
					"size": "32px",
					"slug": "huge"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textTransform": true
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/heading": {
				"typography": {
					"fontSize": "var:preset|font-size|small",
					"fontStyle": "normal",
					"fontWeight": "400"
				}
			}
		},
		"color": {
			"background": "var:preset|color|textonbutton",
			"text": "var:preset|color|textonprimary"
		},
		"elements": {
			"h1": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--huge)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h4": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "5px",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h5": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "8px",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h6": {
				"spacing": {
					"margin": {
						"bottom": "var(--wp--custom--spacing--elements)",
						"top": "var(--wp--custom--spacing--elements)"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--inter)",
					"fontSize": "13px",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "#00a2e8"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--charta--layout-spacing)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--barlow)",
			"fontSize": "var(--wp--custom--typography--font-size--text)",
			"lineHeight": "var(--wp--custom--typography--line-height--text)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "header",
			"name": "headerblack",
			"title": "Dark Simple Header"
		},
		{
			"area": "header",
			"name": "headercenter",
			"title": "Centered Logo"
		},
		{
			"area": "header",
			"name": "headermenubutton",
			"title": "Header with button in menu"
		},
		{
			"area": "header",
			"name": "headersearchbutton",
			"title": "Header with search icon and button"
		},
		{
			"area": "header",
			"name": "headertwomenus",
			"title": "Header with two menus"
		},
		{
			"area": "header",
			"name": "headertransparent",
			"title": "Transparent Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Default Footer"
		},
		{
			"area": "footer",
			"name": "footermega",
			"title": "Footer with button and links"
		},
		{
			"area": "footer",
			"name": "footerwhitecolumns",
			"title": "Simple footer"
		},
		{
			"area": "footer",
			"name": "footerdarkcolumns",
			"title": "Dark simple footer"
		},
		{
			"area": "uncategorized",
			"name": "simpletitle",
			"title": "Title area"
		},
		{
			"area": "uncategorized",
			"name": "titlebackground",
			"title": "Title on grey background"
		},
		{
			"area": "uncategorized",
			"name": "titlecolorbg",
			"title": "Title on colored background"
		},
		{
			"area": "uncategorized",
			"name": "titlecoverbg",
			"title": "Title on cover image"
		}
	],
	"version": 2
}